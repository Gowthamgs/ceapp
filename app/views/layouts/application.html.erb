<!DOCTYPE html>
<html>
  <head>
    <title>Ceapp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div>
<% if use_signed_in? %>
    Welcome <%=  current_use.email %>
    <%= link_to "Sign out", destroy_use_session_path, :method => :delete %>
<% else %>
    <%= link_to "Sign in", new_use_session_path%> or <%= link_to "Sign up", new_use_registration_path %>
<% end %>
</div>
    <%= yield %>
  </body>
</html>
